---
title: home
layout: page
permalink: index.html
---

{% for section in collections.sections %}
    <div class="docs-section" id="{{ section.data.title | slug }}">
        <h3 class="docs-section__title">
            {{ section.data.title }}
        </h3>
        {% if section.data.parameters %}
            <p>{{ section.data.description }}</p>
        {% endif %}
        {% if section.data.function != false %}
            <pre class="unstyle docs-section__function"><code>{{ section.data.title }}{% if section.data.parameters %}({{ section.data.parameters }}){% endif %}</code></pre>
        {% endif %}
        <hr>
        {{ section.templateContent }}
        
        <a class="docs-section__edit d-none d-lg-flex" href="https://github.com/lennertderyck/docs/edit/master/cutlery.js{{ section.inputPath | replace_first: '.', '' }}" target="_blank">
            <div class="edit__label">
                <span class="small">edit on GitHub</span> 
            </div>
            <i class="uil uil-pen"></i>
        </a>
    </div>
{% endfor %}

